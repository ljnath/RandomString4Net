    <SignAssembly>True</SignAssembly>
    <AssemblyOriginatorKeyFile>C:\devtools\keys\code_signing_ljnath.pfx</AssemblyOriginatorKeyFile>
    <IncludeSymbols>False</IncludeSymbols>
